<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AspNetNHibernate.API"
                   namespace="AspNetNHibernate.API.Entities">
  <class name="Products">
    <id name="Id">
      <generator class="native"/>
    </id>
    <property name="Title"/>
    <property name="Category"/>
    <property name="Size"/>
    <property name="Price"/>
  </class>
</hibernate-mapping>